[
    {
        "name": "gin-skeleton",
        "cwd": "./release",
        "script": "./gin-skeleton",
        "args": "",
        "exec_mode": "fork",
        "watch": true,
        "ignore_watch": [
            "config",
            "storage",
            ".gitignore"
        ],
        "watch_options": {
            "usePolling": true
        },
        "min_uptime": "60s",
        "max_memory_restart": "512M",
        "max_restarts": 5,
        "error_file": "./storage/logs/pm2-error.log",
        "out_file": "./storage/logs/pm2-out.log"
    }
]